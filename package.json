{
  "name": "seven-square-bridge",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix vite:build",
    "build:server": "tsx ./server/build-server.ts",
    "deploy": "fly deploy --remote-only",
    "dev": "MOCKS=true tsx ./server/index.ts",
    "dev:unmock": "tsx ./server/index.ts",
    "start": "NODE_ENV=production node ./server-build/index.js",
    "check": "biome check .",
    "check:write": "biome check --write .",
    "check:write-unsafe": "biome check --write --unsafe .",
    "typecheck": "tsc",
    "test:unit": "vitest --ui",
    "test:unit:ci": "vitest run",
    "test:unit:reset-db": "npx prisma migrate reset -f",
    "test:unit:reset-db-dev": "npx prisma migrate reset -f",
    "test:e2e": "start-server-and-test dev http://localhost:3000 \"npx playwright test\"",
    "test:e2e:ui": "start-server-and-test dev http://localhost:3000 \"npx playwright test --ui --ui-host=0.0.0.0 --ui-port=8888\"",
    "validate": "run-s check typecheck test:unit:ci test:e2e"
  },
  "dependencies": {
    "@libsql/client": "^0.6.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/adapter-libsql": "^5.15.0",
    "@prisma/client": "^5.15.0",
    "@remix-run/css-bundle": "^2.9.2",
    "@remix-run/node": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "@remix-run/serve": "^2.9.2",
    "@simplewebauthn/browser": "^10.0.0",
    "@simplewebauthn/server": "^10.0.0",
    "@types/express": "^4.17.21",
    "close-with-grace": "^1.3.0",
    "express": "^4.19.2",
    "isbot": "^5.1.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remix-auth": "^3.7.0",
    "remix-auth-google": "^2.0.0",
    "remix-auth-webauthn": "^0.3.4",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.1",
    "@playwright/test": "^1.44.1",
    "@remix-run/dev": "^2.9.2",
    "@remix-run/testing": "^2.9.2",
    "@simplewebauthn/typescript-types": "^8.3.4",
    "@testing-library/react": "^16.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/fs-extra": "^11.0.4",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^1.6.0",
    "esbuild": "^0.21.5",
    "execa": "^9.3.0",
    "fs-extra": "^11.2.0",
    "happy-dom": "^14.12.0",
    "msw": "^2.3.1",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.15.0",
    "remix-development-tools": "^4.2.0",
    "remix-flat-routes": "^0.6.5",
    "start-server-and-test": "^2.0.4",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.15.5",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": "20"
  }
}
